														<g:MenuItem text="CS" ui:field="CSMenuItem"/>
														<g:MenuItem text="ECE" ui:field="ECEMenuItem"/>
														<g:MenuItem text="IMGD" ui:field="IMGDMenuItem"/>
														<g:MenuItem text="RBE" ui:field="RBEMenuItem"/>
														<g:MenuItem text="MA" ui:field="MAMenuItem"/>
