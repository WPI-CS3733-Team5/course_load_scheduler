										</g:customCell>
										<g:customCell>
											<g:Button ui:field="sortButton">Sort</g:Button>
										</g:customCell>
									</g:row>
								</g:Grid>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:ListBox visibleItemCount="5" width="462px" height="128px" ui:field="coursesListBox"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:Grid width="472px" height="92px">
									<g:row>
										<g:customCell>
											<g:Grid width="425px" height="132px">
												<g:row>
													<g:customCell>
														<g:Label text="Wishlist" ui:field="wishlistLabel"/>
													</g:customCell>
												</g:row>
												<g:row>
													<g:customCell>
														<g:ListBox visibleItemCount="5" width="414px" ui:field="wishlistListbox" height="102px"/>
													</g:customCell>
												</g:row>
											</g:Grid>
										</g:customCell>
										<g:customCell>
											<g:Grid width="309px" height="144px">
												<g:row>
													<g:customCell>
														<g:Button text="Create New Course" ui:field="createCourseButton">Create Course</g:Button>
													</g:customCell>
												</g:row>
												<g:row>
													<g:customCell>
														<g:Button ui:field="removeSelectedFromWishlistButton">Remove Selected From Wishlist</g:Button>
													</g:customCell>
												</g:row>
												<g:row>
													<g:customCell>
														<g:Button text="View Full Wishlist" ui:field="viewFullWishlistButton"/>
													</g:customCell>
												</g:row>
											</g:Grid>
										</g:customCell>
									</g:row>
								</g:Grid>
							</g:customCell>
						</g:row>
						<g:row/>
					</g:Grid>
				</g:customCell>
				<g:customCell>
					<g:Grid width="373px" height="405px">
						<g:row>
							<g:customCell>
								<g:Grid width="395px">
									<g:row>
										<g:customCell>
											<g:Label text="Department:" ui:field="departmentLabel"/>
										</g:customCell>
										<g:customCell>
											<g:TextBox ui:field="departmentTextBox"/>
										</g:customCell>
									</g:row>
									<g:row>
										<g:customCell>
											<g:Label text="Course Number:" ui:field="courseNumberSideLabel"/>
										</g:customCell>
										<g:customCell>
											<g:TextBox ui:field="courseNumberSideTextBox"/>
										</g:customCell>
									</g:row>
									<g:row>
										<g:customCell>
											<g:Label text="Course Section:" ui:field="courseSectionLabel"/>
										</g:customCell>
										<g:customCell>
											<g:TextBox ui:field="courseSectionTextBox"/>
										</g:customCell>
									</g:row>
									<g:row>
										<g:customCell>
											<g:Label text="Section Type:" ui:field="sectionTypeLabel"/>
										</g:customCell>
										<g:customCell>
											<g:TextBox ui:field="sectionTypeTextBox"/>
										</g:customCell>
									</g:row>
									<g:row>
										<g:customCell>
											<g:Label text="Term:" ui:field="termLabel"/>
										</g:customCell>
										<g:customCell>
											<g:TextBox ui:field="termTextBox"/>
										</g:customCell>
									</g:row>
									<g:row>
										<g:customCell>
											<g:Label text="Meeting Times:" ui:field="meetingTimesLabel"/>
										</g:customCell>
										<g:customCell>
											<g:TextBox ui:field="meetingTimesTextBox"/>
										</g:customCell>
									</g:row>
									<g:row>
										<g:customCell>
											<g:Label text="Location:" ui:field="locationLabel"/>
										</g:customCell>
										<g:customCell>
											<g:TextBox ui:field="locationTextBox"/>
										</g:customCell>
									</g:row>
								</g:Grid>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:Grid width="339px">
									<g:row>
										<g:customCell>
											<g:Button width="244px" ui:field="addToWishlistButton">Add To Wishlist</g:Button>
										</g:customCell>
									</g:row>
									<g:row>
										<g:customCell>
											<g:Button width="244px" ui:field="editButton">Edit</g:Button>
										</g:customCell>
									</g:row>
									<g:row>
										<g:customCell>
											<g:Button text="Apply Changes" width="244px" ui:field="applyChangesButton">Apply Changes</g:Button>
										</g:customCell>
										<g:customCell>
											<g:Button width="141px" ui:field="cancelChangesButton">Cancel Changes</g:Button>
										</g:customCell>
									</g:row>
									<g:row>
										<g:customCell>
											<g:Button width="244px" ui:field="removeCourseButton">Remove Course</g:Button>
										</g:customCell>
									</g:row>
								</g:Grid>
							</g:customCell>
						</g:row>
					</g:Grid>
				</g:customCell>
			</g:row>
			<g:row/>
		</g:Grid>

	</g:HTMLPanel> 
	<g:PopupPanel ui:field="popupPanel1" width="164px" height="153px"> <!-- addStyleNames="{style.loading-glass-panel}, {style.gwt-popup-panel-glass}" previewingAllNativeEvents="true"-->
		<g:Grid width="911px" height="475px">
			<g:row>
				<g:customCell>
					<g:Grid>
						<g:row>
							<g:customCell>
								<g:Label text="Sort By:" ui:field="popup1SortByLabel"/>
							</g:customCell>
						</g:row>
					</g:Grid>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Grid>
						<g:row>
							<g:customCell>
								<g:RadioButton name="new name" text="Lecture" ui:field="popup1LectureRadio"/>
							</g:customCell>
						</g:row>
					</g:Grid>
				</g:customCell>
			</g:row>
			<g:row>
