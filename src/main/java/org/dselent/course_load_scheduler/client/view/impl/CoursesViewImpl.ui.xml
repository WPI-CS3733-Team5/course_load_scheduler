												</g:MenuItem>
											</g:MenuBar>
										</g:customCell>
										<g:customCell>
											<g:RadioButton name="new name" ui:field="termRadioButton"/>
										</g:customCell>
										<g:customCell>
											<g:MenuBar ui:field="termMenuBar">
												<g:MenuItem text="Term" ui:field="termMenuItem">
													<g:MenuBar vertical="true">
														<g:MenuItem text="A" ui:field="termA"/>
														<g:MenuItem text="B" ui:field="termB"/>
														<g:MenuItem text="C" ui:field="termC"/>
														<g:MenuItem text="D" ui:field="termD"/>
														<g:MenuItem text="E1" ui:field="termE1"/>
														<g:MenuItem text="E2" ui:field="termE2"/>
													</g:MenuBar>
												</g:MenuItem>
											</g:MenuBar>
										</g:customCell>
										<g:customCell>
											<g:RadioButton name="new name" ui:field="courseNumberRadioButton"/>
