					<g:Button ui:field="popup2CancelButton">Cancel</g:Button>
				</g:customCell>
			</g:row>
		</g:Grid> <!-- addStyleNames="{style.loading-glass-panel}, {style.gwt-popup-panel-glass}" previewingAllNativeEvents="true"-->
