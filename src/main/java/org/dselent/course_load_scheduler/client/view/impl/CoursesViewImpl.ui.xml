					<g:Grid>
						<g:row>
							<g:customCell>
								<g:RadioButton name="new name" text="Lab" ui:field="popup1LabRadio"/>
							</g:customCell>
							<g:customCell>
								<g:Button ui:field="popup1ApplyButton">Apply</g:Button>
							</g:customCell>
						</g:row>
					</g:Grid>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Grid>
						<g:row>
							<g:customCell>
								<g:RadioButton name="new name" text="Special" ui:field="popup1SpecialRadio"/>
							</g:customCell>
							<g:customCell>
