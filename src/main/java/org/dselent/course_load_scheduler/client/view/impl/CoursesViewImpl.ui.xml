				</g:customCell>
				<g:customCell>
					<g:Button text="Apply" ui:field="popup2ApplyButton"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Grid>
						<g:row>
							<g:customCell>
								<g:CheckBox text="Saturday" ui:field="popup2SaturdayCheckbox"/>
							</g:customCell>
