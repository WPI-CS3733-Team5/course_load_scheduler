	<ui:style>

	.loading-glass-panel
	{
		position:fixed;
		width:100%;
		height:100%;
	}
		
	.gwt-popup-panel-glass
	{
		background-color:rgb(0, 0, 0);
		opacity:0.8;
	}
		
	.loading-image
	{
		position:fixed;
		display:block;
    	margin-left:50%;
    	margin-right:50%;
    	z-index: 1;
    	visibility:hidden;
	}
	
	</ui:style>
	<g:HorizontalPanel>
	<g:HTMLPanel ui:field="mainPanel">
		<g:Grid width="785px" height="471px">
			<g:row>
				<g:customCell>
					<g:Grid width="393px" height="386px">
						<g:row>
							<g:customCell>
								<g:Grid width="258px">
									<g:row>
										<g:customCell>
											<g:Label text="Sort By:" width="59px" ui:field="sortByLabel"/>
										</g:customCell>
										<g:customCell>
											<g:RadioButton name="new name" ui:field="departmentRadioButton"/>
										</g:customCell>
										<g:customCell>
											<g:MenuBar ui:field="departmentMenuBar">
												<g:MenuItem text="Department" ui:field="departmentMenuItem">
