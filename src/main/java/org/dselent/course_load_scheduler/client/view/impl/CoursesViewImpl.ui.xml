							</g:customCell>
						</g:row>
					</g:Grid>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:PopupPanel>
	<g:PopupPanel ui:field="popupPanel2" width="148px">
		<g:Grid width="122px" height="100%">
			<g:row>
				<g:customCell>
					<g:Grid>
