<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
		<g:HTMLPanel width="100%" height="100%">
		<g:VerticalPanel>
			<g:HorizontalPanel>
			<g:ListBox ui:field="termDropDown" name="termDropDown"/>
			<g:ListBox name="usernameDropDown" ui:field="usernameDropDown"/>
			<g:Button ui:field="apply">apply</g:Button>
		</g:HorizontalPanel>
		</g:VerticalPanel>
		<g:Grid ui:field="calendarGrid" title="calendarGrid" width="100%" height="192px">
			<g:row>
				<g:customCell>
					<g:ListBox visibleItemCount="5" ui:field="calendarList" name="calendarList" width="100%"/>
				</g:customCell>
			</g:row>
		</g:Grid>
		<g:Grid>
			<g:row>
				<g:customCell>
					<g:Button ui:field="acceptSchedule">Accept Schedule</g:Button>
				</g:customCell>
				<g:customCell>
					<g:Button ui:field="requestDifferentSchedule">Request Different Schedule</g:Button>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder> 