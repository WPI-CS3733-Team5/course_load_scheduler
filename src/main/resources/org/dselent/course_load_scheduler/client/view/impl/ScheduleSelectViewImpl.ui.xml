<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel width="775" height="500">
		<g:Grid width="775px" height="501px" cellPadding="20" ui:field="mainPanel">
			<g:row>
				<g:customCell>
					<g:Grid width="326px" height="460px">
						<g:row>
							<g:customCell>
								<g:Label text="[x] Possible Schedules for [user]" ui:field="numSchedulesLabel"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:ListBox visibleItemCount="20" width="325px" height="316px" ui:field="scheduleListBox"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:Grid ui:field="bottomButtonPanel" width="272px" height="32px">
									<g:row>
										<g:customCell>
											<g:Button ui:field="backButton" text="Back" width="76px"/>
										</g:customCell>
										<g:customCell>
											<g:Button ui:field="deleteButton" text="Delete" width="78px"/>
										</g:customCell>
										<g:customCell>
											<g:Button ui:field="acceptButton" text="Accept" width="79px"/>
										</g:customCell>
									</g:row>
								</g:Grid>
							</g:customCell>
						</g:row>
					</g:Grid>
				</g:customCell>
				<g:customCell>
					<g:Grid width="364px" height="460px">
						<g:row>
							<g:customCell>
								<g:Grid width="246px" height="53px" ui:field="displaySelectPanel">
									<g:row>
										<g:customCell>
											<g:Label text="Display Term: " ui:field="displayTermLabel"/>
										</g:customCell>
										<g:customCell>
											<g:ListBox ui:field="termCombo"/>
										</g:customCell>
										<g:customCell>
											<g:IntegerBox width="75px" text="Year" ui:field="yearBox"/>
										</g:customCell>
									</g:row>
								</g:Grid>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:ListBox visibleItemCount="5" width="363px" height="228px" ui:field="courseListBox"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:Grid width="361px" height="156px" ui:field="calendarPanel"/>
							</g:customCell>
						</g:row>
					</g:Grid>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder> 